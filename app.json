{
  "expo": {
    "name": "ASE Ventas",
    "slug": "ASE-Ventas",
    "privacy": "public",
    "platforms": [
      "ios",
      "android",
      "web"
    ],
    "version": "1.15.0",
    "android": {
        "permissions": [
            "CAMERA", 
            "MEDIA_LIBRARY",
            "READ_EXTERNAL_STORAGE",
            "READ_INTERNAL_STORAGE",
            "WRITE_EXTERNAL_STORAGE"],
      "package": "ar.com.ase.ventas",
      "versionCode": 2,
      "useNextNotificationsApi": true
    },
    "orientation": "portrait",
    "icon": "./assets/AseIconSquare.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": false,
      "bundleIdentifier": "ar.com.ase.ventas",
      "buildNumber": "202106280900",
      "usesIcloudStorage": true,
      "infoPlist": {
        "UISupportsDocumentBrowser": true,
        "UIFileSharingEnabled": true,
        "LSSupportsOpeningDocumentsInPlace": true,
        "NSPhotoLibraryUsageDescription": "Los documentos adjuntos serán recibidos, descargados y guardados, para ser procesados internamente en el alta del nuevo beneficiario en ASE.",
        "NSCameraUsageDescription": "Las imágenes de las fotos de los formularios, serán recibidas y guardadas, para ser procesadas internamente en el alta del nuevo beneficiario en ASE.",
        "CFBundleLocalizations" : ["es"],
        "CFBundleDevelopmentRegion" : "es",
        "CFBundleAllowMixedLocalizations": true
      }
    },
    "locales": {
      "es": "./locales/es.json"
    }
  }
}
